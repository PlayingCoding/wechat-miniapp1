<view class="container">
  <view class="address">
    <view class="location">
      <image src="/static/location.png" />
      <text >东莞</text>
    </view>
    <view >更换地区</view>
  </view>
  <view class="list">
    <view class="item" wx:for="{{list}}" wx:key="id" data-id="{{item.id}}" bindtap="bindTap">
      <view class="info-box">
        <image src="{{item.images}}" />
        <view class="info">
          <view class="username">{{item.name}}</view>
          <view class="year">{{item.work_exp}}年工作经验</view>
          <view class="type">{{item.category_name}}</view>
        </view>
      </view>
      <image class="navigation" src="/static/navigation_icon.png" />
    </view>
  </view>
</view>
