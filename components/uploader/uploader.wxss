.ui_uploader_cell {
  width: 100%;
  padding: 40rpx;
  box-sizing: border-box;
}
.ui_uploader_item{
  float: left;
  position: relative;
  margin-right: 30rpx;
  margin-bottom: 30rpx;
  width: 120rpx;
  height: 120rpx;
  border: 1rpx solid #eee;
  border-radius: 6rpx;
}
.ui_uploader {
  float: left;
  position: relative;
  margin-right: 25rpx;
  margin-bottom: 25rpx;
  width: 120rpx;
  height: 120rpx;
  background-color: #DEE4F3;
  box-sizing: border-box;
  border-radius: 6rpx;
}

.ui_uploader:before {
  content: " ";
  position: absolute;
  width: 4rpx;
  height: 29rpx;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #2186F4;
}

.ui_uploader:after {
  content: " ";
  position: absolute;
  height: 4rpx;
  width: 29rpx;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #2186F4;
}
.ui_uploader_item_icon{
  position: absolute;
  right: -20rpx;
  top: -20rpx;
  background: #fff;
  border-radius: 50%;
}
.ui_uploader_item image {
  width: 100%;
  height: 100%;
}

.ui_cell{
  display: flex;
  position: relative;
  width: 100%;
  height: 100rpx;
  line-height: 60rpx;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #eee;
  transform:translate3d(0rpx, 0px, 0px);
}
.ui_cell_hd{
  width: 40rpx;
  height: 40rpx;
  margin-top: 10rpx;
  margin-right: 10rpx;
}

.ui_cell_ft{
  float: right;
  color: #999;
}
